variable "resource_group_location" {
  default     = "{{.resource_group_location}}"
  description = "Location of the resource group."
}

variable "org_name" {
  type = string
  default = "{{.org_name}}"
}

variable "vm_size" {
  type = string
  default = "{{.vault_vm_size}}"
}

variable "environment" {
  type = string
  default = "{{.env}}"
}

variable "algorithm" {
  type = string
  default = "{{.algorithm}}"
}

 variable "rsa_bits" {
  type = number
  default = {{.rsa_bits}}
}

variable "billing_env" {
  type = string
  default = "{{.billing_env}}"
}
